{
    "name": "webignition/php-basil-transpiler",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/webignition/php-basil-transpiler",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "webignition@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\BasilTranspiler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\BasilTranspiler\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR2",
        "static-analysis": "./vendor/bin/phpstan analyse src tests --level=7",
        "test": "./vendor/bin/phpunit --colors=always",
        "ci": [
            "@composer cs",
            "@composer static-analysis",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "webignition/php-basil-models": ">=0.64,<1",
        "webignition/php-basil-model-validator": ">=0.4.0,<1",
        "webignition/symfony-panther-dom-crawler-navigator": ">=0.8,<1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpstan/phpstan": "^0.11.5",
        "webignition/php-basil-test-identifier-factory": ">=0.1,<1",
        "mockery/mockery": "^1.2",
        "phpstan/phpstan-mockery": "^0.11.3",
        "symfony/panther": "^0.5.1"

    }
}
